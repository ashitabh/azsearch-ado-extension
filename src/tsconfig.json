{
  "compileOnSave": true,
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES6",
    "sourceMap": true,
    "strict": true,
    "strictNullChecks": false,
    "strictPropertyInitialization": false,
    "noImplicitAny": false,
    "removeComments": true,
    "types": ["node"],
    "moduleResolution": "node",
    "esModuleInterop": true
  },
  "include": [
    "./common/*",
  ]
}
